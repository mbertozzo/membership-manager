{
  "name": "membership-manager",
  "version": "1.0.0",
  "description": "A fullstack app to manage membership details to a club",
  "main": "index.js",
  "author": "Matteo Bertozzo",
  "license": "MIT",
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "build": "npx tsc --rootDir src/server --outDir dist/server",
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/server/index.ts",
    "lint": "eslint src/server --ext ts",
    "start": "node dist/server/index.js",
    "start:prod": "yarn build && cross-env NODE_ENV=production node dist/server/index.js"
  },
  "dependencies": {
    "apollo-server-core": "^3.10.0",
    "apollo-server-express": "^3.10.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "helmet": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.7.7",
    "pg": "^8.7.3",
    "pino": "^8.3.0",
    "sequelize": "^6.21.3",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "mysql2": "^2.3.3",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
